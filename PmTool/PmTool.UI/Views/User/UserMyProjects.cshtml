                            <td>
                                @Html.DisplayFor(modelItem => item.Project_name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Project_description)
                            </td>
                        </tr>
                    }
                </table>
            }
        </div>
        <div class="container" id="PmDataCenter">
            @if (Model.DataCenters.Count == 0)
            {

            }
            else
            {
                <h2>Data Center</h2>
                <table class="table table-bordered table-responsive table-hover">
                    <tr>
                        <th>
                            <p>Id del Data Center</p>
                        </th>
                        <th>
                            <p>Nombre del Data Center</p>
                        </th>
                        <th>
                            <p>Descripcion del Data Center</p>
                        </th>
                      
                    </tr>
                    @foreach (var item in Model.DataCenters)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.DataCenter_request_id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Project_name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Project_description)
                            </td>
                        </tr>
                    }
                </table>
            }
        </div>
        <div class="container" id="PmOffice">
            @if (Model.Offices.Count == 0)
            {

            }
            else
            {
                <h2>Office</h2>
                <table class="table table-bordered table-responsive table-hover">
                    <tr>
                        <th>
                            <p>Id del Office</p>
                        </th>
                        <th>
                            <p>Nombre del Office</p>
                        </th>
                        <th>
                            <p>Descripcion del Office</p>
                        </th>
                        
                    </tr>
                    @foreach (var item in Model.Offices)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Office_request_id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Project_name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Project_description)
                            </td>
                        </tr>
                    }
                </table>
            }
        </div>
        <div class="container" id="PmOtherProject">
            @if (Model.OtherProjects.Count == 0)
            {

            }
            else
            {
                <h2>Other Project</h2>
                <table class="table table-bordered table-responsive table-hover">
                    <tr>
                        <th>
                            <p>Id del Other Projec</p>
                        </th>
                        <th>
                            <p>Descripcion del Other Projec</p>
                        </th>
                      
                    </tr>
                    @foreach (var item in Model.OtherProjects)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Other_request_id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Project_description)
                            </td>
                        </tr>
                    }
                </table>
            }
        </div>
    </div>
</body>
</html>


