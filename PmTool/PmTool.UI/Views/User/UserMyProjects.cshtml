
@model PmTool.UI.Models.MyPMProjects
@{
    ViewBag.Title = "MyProjects";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>My Projects</title>
</head>
<body>
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/ProjectStyles.css" />

    <div>
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("Log Off", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("My Account", "MyAccount", "User", new { area = "" }, new { @class = "navbar-brand" })
                </div>
            </div>
        </div>
    </div>
    <div class="container" id="ProjectsContainer">

        <div class="container" id="PmLab">
            @if (Model.Labs.Count == 0)
            {

            }
            else
            {
                <h2>Labs</h2>
                <table class="table table-bordered table-responsive table-hover">
                    <tr>
                        <th>
                            <p>Id del Labs</p>
                        </th>
                        <th>
                            <p>Nombre del Labs</p>
                        </th>
                        <th>
                            <p>Descripcion del Labs</p>
                        </th>
                    </tr>
                    @foreach (var item in Model.Labs)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Lab_request_id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Project_name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Project_description)
                            </td>
                        </tr>
                    }
                </table>
            }
        </div>
        <div class="container" id="PmFactory">
            @if (Model.Factories.Count == 0)
            {

            }
            else
            {
                <h2>Factory</h2>
                <table class="table table-bordered table-responsive table-hover">
                    <tr>
                        <th>
                            <p>Id del Factory</p>
                        </th>
                        <th>
                            <p>Nombre del Factory</p>
                        </th>
                        <th>
                            <p>Descripcion del Factory</p>
                        </th>
                       
                    </tr>
                    @foreach (var item in Model.Factories)
