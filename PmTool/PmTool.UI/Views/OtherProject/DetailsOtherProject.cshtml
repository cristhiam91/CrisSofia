@model PmTool.UI.Models.OtherProjects

@{
    ViewBag.Title = "DetailsOtherProject";
}

<h2>DetailsOtherProject</h2>

<div>
    <h4>OtherProjects</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Other_request_id)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Other_request_id)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Project_description)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Project_description)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Other_requestor_id)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Other_requestor_id)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Other_projectType)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Other_projectType)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Assigned_pm)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Assigned_pm)
        </dd>

    </dl>
</div>
<p>
    @{int x = (Int32)Session["UserType"];}

    @if (x == 1)
    {
        <input type="button" name="GoMenutButton" class="btn-link" value="Home" onclick="location.href='@Url.Action("PmProjects","User")'" />

    }
    else
    {
        if (x == 2)
        {
            <input type="button" name="GoMenutButton" class="btn-link" value="Home" onclick="location.href='@Url.Action("UserMyProjects","User")'" />

        }
        else
        {
            if (x == 3)
            {
                <input type="button" name="GoMenutButton" class="btn-link" value="Home" onclick="location.href='@Url.Action("Index","OtherProject")'" />

            }
            else
            {
                if (x == 4)
                {
                    <input type="button" name="GoMenutButton" class="btn-link" value="Home" onclick="location.href='@Url.Action("Index","OtherProject")'" />

                }
            }
        }
    }
</p>
