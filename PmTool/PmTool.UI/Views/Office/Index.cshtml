                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Number_of_phonebooths)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Square_footage)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Project_description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Scope)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Connection)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditOfficeProject", new { id = item.Office_request_id }) |
                    @Html.ActionLink("Details", "DetailsOfficeProject", new { id = item.Office_request_id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Office_request_id })
                </td>
            </tr>
        }

    </table>
    @{int x = (Int32)Session["UserType"]; }

    @if (x == 3)
    {
        <div>
            @Html.ActionLink("Back", "AdminConsole", "User")
        </div>
    }
    else
    {
        <div>
            @Html.ActionLink("Back", "PmManagerConsole", "User")
        </div>
    }
</div>