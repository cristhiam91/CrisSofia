            <th>
                @Html.DisplayNameFor(model => model.Speed_connection)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Total_number_workstation_copper_ports)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Total_number_workstation_fiber_ports)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number_of_auditoriums)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number_of_A_type_rooms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number_of_B_type_rooms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number_of_C_type_rooms)
