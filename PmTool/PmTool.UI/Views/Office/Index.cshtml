                    @Html.DisplayFor(modelItem => item.Total_number_ports_per_workstation)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Speed_connection)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Total_number_workstation_copper_ports)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Total_number_workstation_fiber_ports)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Number_of_auditoriums)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Number_of_A_type_rooms)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Number_of_B_type_rooms)
