@model IEnumerable<PmTool.UI.Models.Offices>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="table-responsive">
    <p>
        @Html.ActionLink("Create New", "CreateOfficeProject")
    </p>
    <table class="table table-bordered table-responsive table-hover table-condensed">
