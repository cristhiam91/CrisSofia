            </th>
            <th>
                @Html.DisplayNameFor(model => model.Scope)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Connection)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Office_request_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Office_requestor_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Assigned_pm)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Site_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Program_name)
                </td>
